
@if (@Model.Count != 0)
{
  @Html.Partial("found_contact.cshtml");
  <ul>
    @foreach (var contact in Model)
    {
      <li>Name: @contact.GetName()</li>
        <ul>
          <li>Phone Number: @contact.GetPhoneNumber()</li>
          <li>Address: @contact.GetAddress()</li>
        </ul>
    }
  </ul>
}
else
{
  <h2>There is no matching contact</h2>
}

<form action="/">
  <button type="click">Home</button>
</form>
